{
  "manifest_version": 2,
  "name": "bookmarks",
  "version": "1",
  "permissions": ["tabs", "storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["main.js"],
      "css": ["style.css"]
    }
  ],
  "background":{ 
    "scripts": [
        "background.js"
    ]
  }
}
